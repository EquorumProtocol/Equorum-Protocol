# ============================================
# DEPLOYMENT CONFIGURATION
# ============================================

# Your wallet private key (NEVER share this!)
# Get from MetaMask: Settings > Security & Privacy > Show Private Key
PRIVATE_KEY=your_private_key_here

# ============================================
# RPC ENDPOINTS (Optional - has defaults)
# ============================================

# Arbitrum Sepolia Testnet (default: https://sepolia-rollup.arbitrum.io/rpc)
ARBITRUM_SEPOLIA_RPC_URL=https://sepolia-rollup.arbitrum.io/rpc

# Arbitrum Mainnet (default: https://arb1.arbitrum.io/rpc)
ARBITRUM_MAINNET_RPC_URL=https://arb1.arbitrum.io/rpc

# ============================================
# TOKEN ALLOCATION ADDRESSES
# ============================================

# Address to receive 500K liquidity tokens (default: deployer address)
LIQUIDITY_ADDRESS=

# Address for Genesis vesting (3M tokens over 72 months) (default: deployer address)
GENESIS_ADDRESS=

# ============================================
# BLOCK EXPLORERS (For contract verification)
# ============================================

# Arbiscan API Key (get from: https://arbiscan.io/myapikey)
ARBISCAN_API_KEY=

# Etherscan API Key (optional, for Ethereum Sepolia)
ETHERSCAN_API_KEY=

# ============================================
# GAS REPORTING (Optional)
# ============================================

# Enable gas reporting (true/false)
REPORT_GAS=false

# CoinMarketCap API Key (for USD gas prices)
COINMARKETCAP_API_KEY=
